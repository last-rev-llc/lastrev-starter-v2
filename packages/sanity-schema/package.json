{
  "name": "sanity-schema",
  "version": "0.1.0",
  "private": true,
  "main": "schema.js",
  "types": "schema.d.ts",
  "files": [
    "schema.js",
    "schema.d.ts"
  ],
  "scripts": {
    "remove-tables": "",
    "sanity-contentful-export": "contentful-to-sanity export -s $CONTENTFUL_STARTER_SPACE_ID -t $CONTENTFUL_MANAGEMENT_TOKEN -a $CONTENTFUL_STARTER_ACCESS_TOKEN . && node scripts/removeTables.js",
    "sanity-create-schema": "contentful-to-sanity schema --keep-markdown --intl multiple --schema-file schema.js . && node scripts/fixSchemaErrors.js",
    "sanity-create-dataset": "contentful-to-sanity dataset --keep-markdown --locale en-US .",
    "sanity-dataset-import": "pnpm sanity dataset import ./dataset.ndjson $SANITY_STUDIO_SANITY_DATASET --replace"
  },
  "dependencies": {
    "sanity": "^3.90.0"
  },
  "devDependencies": {
    "contentful-to-sanity": "^4.2.10",
    "eslint": "^9.9.0",
    "prettier": "^3.0.2"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "bracketSpacing": false,
    "singleQuote": true
  }
}
